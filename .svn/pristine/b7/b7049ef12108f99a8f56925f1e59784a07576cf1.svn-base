    //初始化DataConnsAndFunctionsList
	function initDataConnsAndFunctionsList(dataConns,functionsList){
		//初始化.connectionEvent和#sjlj
		var str='<option value="" >请选择数据连接</option>';
        $(dataConns).each(function(index,item){
            str+='<option value='+item.ID+'>'+item.NAME+'</option>';
        });
        $('.connectionEvent').html(str);
        $('#sjlj').html(str);
      	//初始化.syb_con和#showTip_jsmind_container2
		var functionName = null;
		var formula = null;
		var placeholders = null;
		var parameters = new Array();
		var str = null;
		var number;
		var fixed;
		var columnType;
		var sysdoms = '<p fk = "0" textValue = "?+?+..." level="6" functionName="+" paramNumber="3" fixed="0" class="draggable">add</p>'+
		'<p fk = "0" textValue = "subtraction" level="6" functionName="-" paramNumber="3" fixed="0" class="draggable">subtraction</p>'+
		'<p fk = "0" textValue = "multiplication" level="6" functionName="*" paramNumber="3" fixed="0" class="draggable">multiplication</p>'+
		'<p fk = "0" textValue = "division" level="6" functionName="/" paramNumber="3" fixed="0" class="draggable">division</p>'+
		'<p fk = "0" textValue = "remainder" level="6" functionName="%" paramNumber="3" fixed="0" class="draggable">remainder</p>'+
		'<p fk = "0" textValue = "Constant" level="5" functionName="?" paramNumber="1" fixed="1" class="draggable">constant</p>';
		$(functionsList).each(function(index,item){
			functionName = item.name;
			parameters = item.parameters;
			functionsMap.put(functionName,parameters);
			if(parameters.length>0){
				number = parameters.length-1;
				placeholders=new Array();
				columnType = parameters[number].paramDataType;
			}
			fixed=item.fixed;
			/* $(parameters).each(function(i,element){
				if(i>0){
					placeholders[i-1] = "?";
				}
			});
			if(fixed==0){
				placeholders[parameters.length-2] = "...";
			}
			//str = placeholders.join().replace(/\"/g,"");
			str = placeholders.join();
			formula = functionName+"("+str+")";  */
			var sysdom = '<p fk = "0" textValue = "'+functionName+'" level="3" functionName="'+functionName+'" columnType="'+columnType+'" paramNumber="'+number+'" fixed="'+fixed+'" class="draggable">'+functionName+'</p>';
			sysdoms+=sysdom;
		});
		$('.syb_con').html($(sysdoms));
		$('#showTip_jsmind_container2').html($(sysdoms));
	}
    function addMenu(menuId,menuType,name){
        menuId = filterString(menuId);
		var iconType;
		//2是controller，3是flow，4是datamining，5是biIcon
		if(menuType==2){
			iconType='controllerIcon';
		}else if(menuType==3){
			iconType='flowIcon';
		}else if(menuType==4){
			iconType='DataMicon';
		}else if(menuType==5){
			iconType='biIcon';
		}
        $.ajax({
            url: urlId + '/addmenu',
            type: "POST",
            dataType:"json",
            data:{
                menuId:menuId,
                userId:userId,
                name:name,
                menuType:menuType,
                power:maxPower
            },
            success:function(data) {
                //console.log(data);
                var isShow = showType(maxPower);
                if(menuId==0&&menuType==menuTypeEnum[0]){
                    $('.root_tree').append($('<li data-title="'+ name + '"><div id="'+data+'" menuType="'+menuType+'" power="'+maxPower+'" name="'+isShow+'" class="folder_self" onclick="getUserPower('+data+','+menuType+');"><i class="fa fa-folder-open_ folder_title"></i><span class="rootTitle" style="margin-left: 4px">' + name + '</span></div><ul></ul></li>'));
                    renameMenuForHome();
                }else if(menuId==0&&menuType!=menuTypeEnum[0]){
                    $('.root_tree').append($('<li><div id="'+data+'" menuType="'+menuType+'" power="0b01111" name="'+isShow+'" class="folder_child" onclick="getUserPower('+data+','+menuType+');"><i class="fa '+ iconType +'"></i><span>' + name + '</span></div></li>'));
                    renameMenuForHome();
                }else if(menuId!=0&&menuType==menuTypeEnum[0]){
                    $currentRightDom.next().append($('<li data-title="'+ name + '"><div id="'+data+'" menuType="'+menuType+'" power="'+maxPower+'" name="'+isShow+'" class="folder_self" onclick="getUserPower('+data+','+menuType+');"><i class="fa fa-folder-open_ folder_title"></i><span>' + name + '</span></div><ul></ul></li>'));
                    renameMenu();
                }else{
                    $currentRightDom.next().append($('<li><div id="'+data+'" menuType="'+menuType+'" power="0b01111" name="'+isShow+'" class="folder_child" onclick="getUserPower('+data+','+menuType+');"><i class="fa ' + iconType + '"></i><span>' + name + '</span></div></li>'));
                    renameMenu();
                }
                initRightEvent();
                initFolderClick();

            },
            error : function() {
                $.dialog("异常！");
