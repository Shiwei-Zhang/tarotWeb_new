    var refreshEnum = [1,2];
    //公共对象：标记枚举
    var signArray = new Array();
    //公共对象：权限枚举
    var powerEnum = ["0b10000","0b01000","0b00100","0b00010","0b00001"];
    //公共对象：最高权限
    var maxPower = "0b11111";
    //公共对象：目录类型枚举，1是文件夹，2是controller，3是flow，4是datamining，5是templet
    var menuTypeEnum = [1,2,3,4,5];
