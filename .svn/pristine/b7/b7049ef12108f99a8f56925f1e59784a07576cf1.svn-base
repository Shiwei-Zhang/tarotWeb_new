    function getFlowIdByMenuId(menuId){
    	$.ajax({
			url:urlId+'/getFlowIdByMenuId?menuId='+menuId,
			type:'POST',
			dataType:"json",
			contentType: "application/json",
			success:function(data) {
				if(flowId_global){
					var jsmindStr = create_jsmindStr();
					jsmindStrsMap.put(flowId_global,jsmindStr);
					clear_panel_();
				}
				/********************以上是保存上一个flow并清空，以下是展现下一个flow**********************/
				//赋值全局变量flowId_global
				flowId_global = data;
				var jsmindStr_new = jsmindStrsMap.get(flowId_global);
				var sourceDatas = sourceDatasMap.get(flowId_global);
				if(!jsmindStr_new){
					var jsmindAndSourcedatas = getSelectAllFlowId(flowId_global);
					jsmindStr_new = jsmindAndSourcedatas.jsmindStr;
					sourceDatas = jsmindAndSourcedatas.sourceDatas;
					jsmindStrsMap.put(flowId_global,jsmindStr_new);
					sourceDatasMap.put(flowId_global,sourceDatas);
				}
				//初始化jsmind
				var jsmind = JSON.parse(jsmindStr_new);
				if(jsmind){
					_jm1.show(jsmind);	
					var dataList = jsmind.data;
					$.each(dataList,function(index,item){
						if(!item.isroot){
							var pk = item.pk;
							subtreeMap.put(pk,item.subMind);
				    		paramsMap.put(pk,item.params);
				    		joinsMap.put(pk,item.joins);
				    		targetsMap.put(pk,item.saveTargets);
						}
					});
					var nodes = _jm1.mind.nodes;
					var node = null;
					for(var nodeid in nodes){
						node = nodes[nodeid];
						if(!node.isroot){
							var isDebug = node.data.isDebug;
							var isInterface = node.data.isInterface;
							var isCheck = node.data.isCheck;
							if(isDebug==1){
								$($($('jmnode[nodeid="'+nodeid+'"]')[0]).find('div')[0]).css('visibility','visible');
							}
							if(isCheck==1){
								$($($('jmnode[nodeid="'+nodeid+'"]')[0]).find('div')[1]).css('visibility','visible');
							}
							if(isInterface==1){
								$($($('jmnode[nodeid="'+nodeid+'"]')[0]).find('div')[2]).css('visibility','visible');
							}
						}
					}
				}
				//初始化.dt_conL
				var fileName;
				var sourceDataId;
				var data_obj;
				var line_color = _jm1.options.view.line_color;
				$(sourceDatas).each(function(index,item){
					fileName=item.fileName;
					sourceDataId=item.id;
					data_obj = $(
							"<p textValue = '"+fileName+"' level='2' flowId='"+flowId_global+"' sourceId='"+sourceDataId+"' line_color='"+line_color+"' class='draggable'>"+fileName+"</p>"
					);
					$('.dt_conL').append(data_obj);
					tableColumnsMap.put(sourceDataId,item.columnList);
				});				
				initDragable();
			},
			error : function() {
				$.dialog("异常！");
			}
		})
    }
