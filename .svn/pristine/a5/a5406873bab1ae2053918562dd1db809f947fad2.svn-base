/*Controller start*/

.controll-bodyer{
	/*width: 76%;*/
	height: 700px;
}
.controll-bodyer .content1{
	width: 100%;
	height: 509px;
	padding: 20px 0;
	border-bottom: 1px solid #e7e7e7;
	overflow: hidden;
}
.content1 .stream,.content1 .provide,.content1 .con-right{
	width: 386px;
	height: 509px;
	float: left;
	margin-left: 26px;
	/*background: #f5f5f5;*/
}

.content1 .con-title{
	/*width: 100%;*/
	height: 40px;
	padding: 0 10px;
	background: #F4F4F4;
}
.con-title span{
	display: inline-block;
	height: 40px;
	line-height: 40px;
	float: left;
}
.con-title i{
	display: inline-block;
	width: 32px;
	height: 32px;
	float: right;
	background: url(../images/plus02.png)no-repeat 13px 5px;
	margin-top: 5px;
	background-size: 16px 16px;
}
.stream .stream-con,.con-right .conList{
	position: relative;
	height: 467px;
	border:1px solid #CAC9C9;
}
.stream-con .part,.conList .part{
	max-height: 410px;
	overflow: hidden;
}
