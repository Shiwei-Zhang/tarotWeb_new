*{
	margin: 0;
	padding: 0;
	font-family: 'arial', 'MicrosoftYaHei';
}
ul li{
	list-style: none;
}
input{
	outline: none;
}
.clearFix{
	display: block;
	clear: both;
	content: '';
}
.header_active {
	background-color: #C8C8C8;
	border: 1px solid #979797;
}

.manager-bodyer {
	width: 85%;
}

#manag {
	width: 100px;
	height: 100px;
	background-color: #FFF;
	position: absolute;
	z-index: 100;
	top: 63px;
	right: 0px;
	display: none;
	text-align: center;
	border-left: 1px #b6bccc solid;
	border-bottom: 1px #b6bccc solid;
	border-right: 1px #b6bccc solid;
}

#manag h6 {
	width: 100%;
	font-weight: normal;
	margin-top: 10px;
	margin-bottom: 10px;
	cursor: pointer;
}

#manag h6:hover {
	background: #87CEFF;
}

.center-table1, .center-table2, .power-detail {
	width: 98.5%;
	height: 693px;
	border: 10px #F4F4F4 solid;
	display: none;
}

.search {
	background: #EAEAEA;
	/*border: 1px #F0F0F0 solid;*/
	padding: 13px;
	position: relative;
	width: 98%;
	height: 34px;
}
.search .searchInfo{
	display: none;
	width: 448px;
	/*height: 289px;*/
	max-height: 289px;
	overflow: hidden;
	background: #fff;
	box-shadow: 0 2px 2px 2px #f4f4f4;
}
.search .searchInfo li{
	height: 35px;
	line-height: 35px;
	padding-left: 20px;
	font-size: 14px;
	cursor: pointer;
}
.search .searchInfo li:hover{
	background: #e7e7e7;
}
.fa-search {
	color: #888;
	z-index: 100;
	position: absolute;
	top: 23px;
	left: 21px;
	font-size: 15px;
}

.sou1 {
	width: 420px;
	height: 32px;
	font-size: 14px;
	font-family: 'arial', 'MicrosoftYaHei';
	border: 1px #C1CDCD solid;
	padding-left: 28px;
}

.search-text {
	height: 34px;
	width: 80px;
	color: #FFFFFF;
	font-weight: 600;
	margin-left: 10px;
	background-color: #5CACEE;
	border: 1px solid #5CACEE;
}

.table1, .table2, .table3 {
	word-break: break-all;
	width: 100%;
	height: 588px;
	border: 1px #F0F0F0 solid;
	color: #1E1E1E ;
	font-family: verdana, arial, sans-serif;
	font-size: 14px;
	border-collapse: collapse;
}

.table2 tr th{
	width: 288px;
	height: 50px;
	font-size: 14px;
	text-align: left;
	padding-left: 20px;
	color:  #1E1E1E ;
	background-color: #fff;
}
.table3 tr th {
	width: 288px;
	height: 50px;
	font-size: 14px;
	text-align: left;
	padding-left: 20px;
	color:  #1E1E1E ;
	background-color: #fff;
}
.table1 tr th{
	height: 50px;
	font-size: 14px;
	color:  #1E1E1E ;
	background-color: #fff;
	text-align: left;
}
.table1 tr td{
	height: 48px;
	font-size: 14px;
	color:  #1E1E1E ;
	border-bottom: 1px solid #D1D6D6;
	border-left: 1px solid #D1D6D6;
	/*text-align: center;*/
}
.table1 tr td input{
	width: 14px;
	height: 14px;
}
.table2 tr td, .table3 tr td {
	width: 288px;
	height: 48px;
	font-size: 14px;
	text-align: left;
	padding-left: 20px;
	border-bottom: 1px solid #D1D6D6;
	border-left: 1px solid #D1D6D6;
	/*text-align: center;*/
}

.table2 tr td.w100,.table2 tr th.w100{
	width: 90px;
	position: relative;
}
.table1 .firTr,.table2 .firTr{
	display: block;
	border:1px solid #D1D6D6;
	border-bottom: 1px solid #A0A0A0
}

.borRight{
	border-right: 1px solid #D1D6D6;
}
/*table1 width*/
.w180{
	width: 154px;
	padding-left:20px;
}
/*增删改查width*/
.w50{
	width: 50px;
	text-align: center;
}
.w50 input{
	cursor: pointer;
}
.add2 {
	position: absolute;
	right: 13px;
	cursor: pointer;
}

.presh {
	position: relative;
}

.table2 .mark{
	display: inline-block;
	width: 40px;
	height: 20px;
	line-height: 20px;
	margin-left: 16px;
	background: url("../images/more.png")no-repeat center center;
	background-size: 40px 20px ;
	vertical-align: bottom;
}
.fa-tasks {
	cursor: pointer;
	position: relative;
	top: 0px;
	right: 0px;
}

.task {
	display: none;
	position: absolute;
	right: 31px;
	top: 44px;
	z-index: 100;
	width: 140px;
	height: 48px;
	padding: 16px 0;
	background-color: #FFF;
	border: 1px #F0F0F0 solid;
	box-shadow: 0 0 3px 0 #e3e3e3;
}

.task div {
	height: 24px;
	line-height: 24px;
	padding-left: 20px;
	cursor: pointer;
}

.task div:hover {
	background: #87CEFF;
	color: #feffff;
}

.task .look:before{
	position: absolute;
	top: -20px;
	right: 10px;
	width: 0;
	height: 0;
	border: 10px solid transparent;
	border-bottom-color: #fff;
	content: '';
}
.add2-sel, .edit-sel, .add2-f table input, .look-detail table input,
	.edit-conn table input {
	width: 465px;
	height: 34px;
	border: 0px;
}

.look-button {
	height: 60px;
}

.edit-button {
	height: 60px;
	display: none;
}

.table2-but {
	position: relative;
	left: 488px;
	bottom: -9px;
	background-color: #fff;
	color: #5CACEE;
	border-color: #5CACEE;
	height: 27px;
	width: 51px;
}

.add2-f .h4, .look-detail .h4, .edit-conn .h4 {
	background-color: #F5FFFA;
	height: 18px;
	padding: 18px 0 18px 25px;
	font-family: "Times New Roman", Georgia, Serif;
}

.fts {
	position: relative;
	left: 501px;
	bottom: 3px;
	cursor: pointer;
}

.table1-save {
	background-color: #5CACEE;
	height: 70%;
	width: 65%;
	font-weight: 600;
	color: #FFFFFF;
	border: 1px solid #ccc;
}

.add2-f, .look-detail, .edit-conn {
	width: 646px;
	min-height: 300px;
	height: 500px;
	background-color: #FFF;
	position: absolute;
	z-index: 101;
	top: 130px;
	left: 434px;
	display: none;
	border: 1px #F0F0F0 solid;
}

.add2-f table, .look-detail table, .edit-conn table {
	font-family: verdana, arial, sans-serif;
	font-size: 11px;
	color: #333333;
	border: 1px #DEDEDE solid;
	border-collapse: collapse;
	margin: 20px 0px 17px 27px;
}

.add2-f table th, .look-detail table th, .edit-conn table th {
	border: 1px #DEDEDE solid;
	font-weight: 100;
	width: 100px;
	text-align: left;
	padding: 8px;
	height: 40px;
}

.add2-f table td, .look-detail table td, .edit-conn table td {
	border: 1px #DEDEDE solid;
	padding: 8px;
	width: 465px;
	height: 40px;
	background-color: #ffffff;
}

.add2-table2 {
	height: 300px;
	overflow-y: auto;
	overflow-x: hidden;
}

.fenye {
	width: 100%
}

.barcon {
	text-align: center;
	width: 100%;
	margin: 7px 0;
}

.barcon button {
	background-color: #fff;
	border: none;
	border: 1px solid #ccc;
	padding: 0 8px;
	line-height: 25px;
	margin: 0 5px;
}

.barcon input {
	width: 50px;
}

.barcon  button.active, .barcon button:active {
	background-color: #5CACEE;
	border-color: #5CACEE;
	color: #fff;
}

.barcon button.disabled {
	background-color: #EBEBEB !important;
	color: #999;
}

.barcon button.disabled:active {
	background-color: #EBEBEB !important;
	border-color: #ccc;
	color: #999;
}

tr:nth-child(even) {
	background: #FAFAFA;
}
