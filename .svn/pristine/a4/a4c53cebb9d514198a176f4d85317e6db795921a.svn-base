    function getFlowIdByMenuId(menuId){
    	$.ajax({
			url:urlId+'/getFlowIdByMenuId?menuId='+menuId,
			type:'POST',
			dataType:"json",
			contentType: "application/json",
			success:function(data) {
				if(flowId_global){
					var flowInfo = flowInfosMap.get(flowId_global);
					flowInfo.clickTab = $('#tabbox2 li[class="thistab"]').find('a').attr('tab');
					flowInfo.codeOrChart = $('.btns .selected').val();
					if(!flowInfo.editable){
						window.clearInterval(flowInfo.log_timer);
				    	window.clearInterval(flowInfo.checkaccept_timer);
					}else{
						var jsmindStr = create_jsmindStr();
						jsmindStrsMap.put(flowId_global,jsmindStr);
					}
					clear_panel_flow();
				}
				/********************以上是保存上一个flow并清空，以下是展现下一个flow**********************/
				//赋值全局变量flowId_global
				flowId_global = data;
				var jsmindStr_new = jsmindStrsMap.get(flowId_global);
				var sourceDatas = sourceDatasMap.get(flowId_global);
				var udfInfosMap_min = udfInfosMap.get(flowId_global);
				if(!jsmindStr_new){
					var jsmindAndSourcedatas = getSelectAllFlowId(flowId_global);
					jsmindStr_new = jsmindAndSourcedatas.jsmindStr;
					sourceDatas = jsmindAndSourcedatas.sourceDatas;
					var udfInfos = jsmindAndSourcedatas.udfInfos;
					udfInfosMap_min = new Map();
					$.each(udfInfos,function(index,item){
						udfInfosMap_min.put(item.columnid,item);
					});
					jsmindStrsMap.put(flowId_global,jsmindStr_new);
					sourceDatasMap.put(flowId_global,sourceDatas);
					udfInfosMap.put(flowId_global,udfInfosMap_min);
				}
				//初始化jsmind
				var jsmind = JSON.parse(jsmindStr_new);
				if(jsmind){
					_jm1.show(jsmind);	
					var dataList = jsmind.data;
					$.each(dataList,function(index,item){
						if(!item.isroot){
							var pk = item.pk;
							subtreeMap.put(pk,item.subMind);
				    		paramsMap.put(pk,item.params);
				    		joinsMap.put(pk,item.joins);
				    		targetsMap.put(pk,item.saveTargets);
						}
					});
					var nodes = _jm1.mind.nodes;
					var node = null;
					for(var nodeid in nodes){
						node = nodes[nodeid];
						if(!node.isroot){
							var isDebug = node.data.isDebug;
							var isInterface = node.data.isInterface;
							var isCheck = node.data.isCheck;
							if(isDebug==1){
								$($($('jmnode[nodeid="'+nodeid+'"]')[0]).find('div')[0]).css('visibility','visible');
							}
							if(isCheck==1){
								$($($('jmnode[nodeid="'+nodeid+'"]')[0]).find('div')[1]).css('visibility','visible');
							}
							if(isInterface==1){
								$($($('jmnode[nodeid="'+nodeid+'"]')[0]).find('div')[2]).css('visibility','visible');
							}
						}
					}
				}
				//初始化运行态
				var flowInfo = flowInfosMap.get(flowId_global);
				if(!flowInfo){
					var newflowInfo = new Object();
					newflowInfo.dataMap = new Map();
					newflowInfo.log = "";
					newflowInfo.editable = true;
	            	flowInfosMap.put(flowId_global,newflowInfo);
	            	$('#tabbox2 a[tab="tab2_1"]').parent().click();
	            	$('.btns input[class="chart"]').click();
	            	open_new_flow();
				}else{
					if(flowInfo.clickTab){
						$('#tabbox2 a[tab="'+flowInfo.clickTab+'"]').parent().click();
					}
					if(flowInfo.codeOrChart){
						$('.btns input[value="'+flowInfo.codeOrChart+'"]').click();
					}
					if(!flowInfo.editable){
						$('.module-prop .save').attr('disabled','disabled');
	            		$('.debug,.runing').addClass('selected');
	            		$('.new_table_name input').attr('disabled','disabled');
	            		$('.new_column_name input').attr('disabled','disabled');
	            		_jm1.disable_edit();
	            		_jm2.disable_edit();
	            		if(flowInfo.runType==3){
	            			checkaccept_timer(flowInfo,flowInfo.uuid);
	            		}
	            		log_timer(flowInfo,flowInfo.uuid);
					}else{
						open_new_flow();
					}
				}
				//初始化.dt_conL
				var fileName;
				var sourceDataId;
				var is_interface;
				var data_obj;
				var sourceDataType;
				var line_color = _jm1.options.view.line_color;
				$(sourceDatas).each(function(index,item){
					var sdList = item.sdList;
					var liStr="<li conId='"+item.conId+"'><div class='titleT clearFix'><span>"+item.conName+"</span><i class='arrowUp'></i></div><ul class='detail show'>";
					var liStr_min="";
					$.each(sdList,function(i,element){
						fileName=element.fileName;
						is_interface=element.isInterface;
						sourceDataType=element.sourceDataType;
						if(( sourceDataType == 'csv' ||  sourceDataType == 'ftp'  || sourceDataType == 'hdfs' ) && fileName){
							fileName = fileName.substr(fileName.lastIndexOf('%252F')+5);
						}
						sourceDataId=element.id;
						tableColumnsMap.put(sourceDataId,element.columnList);
						liStr_min+="<li textValue = '"+fileName+"' level='2' flowId='"+flowId_global+"' sourceDataType='"+sourceDataType+"' sourceId='"+sourceDataId+"' is_interface='"+is_interface+"' line_color='"+line_color+"' class='draggable'><i class='branchIcon'></i><span title="+fileName+">"+fileName+"</span></li>";
					});
					liStr+=liStr_min+"</ul></li>";
					var $liStr = $(liStr);
					$.slideStyle($liStr);
					$('#_source_add .menuStyle').append($liStr);
				});
				$('#_source_add .draggable').unbind('mousedown').mousedown(function(e, a){
	        	    if(e.which == 3 || a == 'right'){
	        	        $currentRightDom = $(this);
	        	        generateFun(null,"#_source_add .draggable",0);
	        	    }
	        	});
				initDragable();
				//初始化上下拖拽
				initDragable_updown();
			},
			error : function() {
				$.dialog("获取工作流信息异常！");
			}
		})
    }
